#to add remote address to a repository
git remote add origin <remote address>

#to verify remote repo
git remote -v

#pull
git pull origin <branch name>

#to create branch
git branch <branch name>

#to change branch
git checkout <branchname>

# to check status
git status

#to stage a file
git add <file name>

#commit
git commit <file name>

#commit msg
CTRL+X CTRL+S to save then CTRL+X CTRL+C to quit or if it's vi: :wq

#push
git push origin <branch name>
